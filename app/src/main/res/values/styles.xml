<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="TextLabel" parent="TextAppearance.Design.Hint">
        <item name="android:textSize">16sp</item>
        <item name="paddingStart">0dp</item>
    </style>

    <style name="EditText" parent="ThemeOverlay.Material3.MaterialTimePicker.Display.TextInputEditText">
        <item name="android:paddingStart">16dp</item>
    </style>

    <style name="circle_profile_image">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>
    <style name="movie_ticket_image">
        <item name="cornerSizeTopLeft">@dimen/margin_medium_3</item>
        <item name="cornerSizeTopRight">@dimen/margin_medium_3</item>
    </style>

    <style name="shapeable_overlay_movie">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">@dimen/margin_medium_2</item>
    </style>
    <style name="custom_ratingbar" parent="android:Widget.RatingBar">
        <item name="android:minHeight">24dp</item>
        <item name="android:maxHeight">24dp</item>
    </style>
    <style name="countTextView">
        <item name="strokeColor">@color/secondaryTextColor</item>
        <item name="strokeWidth">2dp</item>
    </style>


</resources>